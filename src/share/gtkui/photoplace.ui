<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkListStore" id="liststore-exifmodes">
    <columns>
      <!-- column-name name -->
      <column type="gchararray"/>
      <!-- column-name mode -->
      <column type="gint"/>
    </columns>
  </object>
  <object class="GtkListStore" id="treestore-photoinfo">
    <columns>
      <!-- column-name deletable -->
      <column type="gboolean"/>
      <!-- column-name editable -->
      <column type="gboolean"/>
      <!-- column-name key -->
      <column type="gchararray"/>
      <!-- column-name value -->
      <column type="gchararray"/>
      <!-- column-name fgcolor -->
      <column type="gchararray"/>
    </columns>
  </object>
  <object class="GtkListStore" id="liststore-geophotos">
    <columns>
      <!-- column-name status -->
      <column type="gint"/>
      <!-- column-name name -->
      <column type="gchararray"/>
      <!-- column-name path -->
      <column type="gchararray"/>
      <!-- column-name date -->
      <column type="gchararray"/>
      <!-- column-name active -->
      <column type="gboolean"/>
      <!-- column-name pixbuf -->
      <column type="GdkPixbuf"/>
      <!-- column-name gettext_main_info -->
      <column type="gchararray"/>
      <!-- column-name gettext_geo_info -->
      <column type="gchararray"/>
      <!-- column-name gettext_ext_info -->
      <column type="gchararray"/>
      <!-- column-name bgcolor -->
      <column type="gchararray"/>
    </columns>
  </object>
  <object class="GtkWindow" id="window">
    <property name="width_request">850</property>
    <property name="height_request">550</property>
    <property name="title">PhotoPlace</property>
    <property name="icon">photoplace.png</property>
    <signal name="destroy" handler="on_window_destroy"/>
    <child>
      <object class="GtkVBox" id="vbox">
        <property name="visible">True</property>
        <child>
          <object class="GtkMenuBar" id="menubar">
            <property name="visible">True</property>
            <child>
              <object class="GtkMenuItem" id="menuitem-file">
                <property name="visible">True</property>
                <property name="label" translatable="yes" context="PhotoPlaceGUI" comments="menuitem">_File</property>
                <property name="use_underline">True</property>
                <child type="submenu">
                  <object class="GtkMenu" id="menu-file">
                    <property name="visible">True</property>
                    <child>
                      <object class="GtkImageMenuItem" id="imagemenuitem-opendir">
                        <property name="label">Open _Directory</property>
                        <property name="visible">True</property>
                        <property name="tooltip_text" translatable="yes" context="PhotoPlaceGUI" comments="menuitem">Directory with photos for geotagging. All photos will be geotagged if &lt;geotagged&gt; mode is selected.</property>
                        <property name="use_underline">True</property>
                        <property name="image">image-opendir</property>
                        <property name="use_stock">False</property>
                        <property name="always_show_image">True</property>
                        <signal name="activate" handler="on_imagemenuitem-opendir_activate"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkImageMenuItem" id="imagemenuitem-opengpx">
                        <property name="label" translatable="yes">Open _GPX</property>
                        <property name="visible">True</property>
                        <property name="tooltip_text" translatable="yes" context="PhotoPlaceGUI" comments="menuitem">Open an eXchange Format (GPX) in order to match the date of each photo with the position.</property>
                        <property name="use_underline">True</property>
                        <property name="image">image-opengpx</property>
                        <property name="use_stock">False</property>
                        <signal name="activate" handler="on_imagemenuitem-opengpx_activate"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkImageMenuItem" id="imagemenuitem-save">
                        <property name="label">_Save as</property>
                        <property name="visible">True</property>
                        <property name="tooltip_text" translatable="yes" context="PhotoPlaceGUI" comments="menuitem">Output file to save the KML data.</property>
                        <property name="use_underline">True</property>
                        <property name="image">image-save</property>
                        <property name="use_stock">False</property>
                        <property name="always_show_image">True</property>
                        <signal name="activate" handler="on_imagemenuitem-save_activate" object="imagemenuitem-save"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkSeparatorMenuItem" id="separatormenuitem-sep1">
                        <property name="visible">True</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkImageMenuItem" id="imagemenuitem-new">
                        <property name="label" translatable="yes" context="PhotoPlaceGUI">_Reset</property>
                        <property name="visible">True</property>
                        <property name="tooltip_text" translatable="yes" context="PhotoPlaceGUI" comments="menuitem">Remove all loaded files/directories to start again.</property>
                        <property name="use_underline">True</property>
                        <property name="image">image-clear</property>
                        <property name="use_stock">False</property>
                        <property name="always_show_image">True</property>
                        <signal name="activate" handler="on_imagemenuitem-new_activate"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkSeparatorMenuItem" id="separatormenuitem-sep2">
                        <property name="visible">True</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkImageMenuItem" id="imagemenuitem-exit">
                        <property name="label" context="PhotoPlaceGUI">gtk-quit</property>
                        <property name="visible">True</property>
                        <property name="tooltip_text" translatable="yes" context="PhotoPlaceGUI" comments="menuitem">Exit the program. I hope see you soon :-)</property>
                        <property name="right_justified">True</property>
                        <property name="use_underline">True</property>
                        <property name="use_stock">True</property>
                        <property name="always_show_image">True</property>
                        <signal name="activate" handler="on_imagemenuitem-exit_activate"/>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkMenuItem" id="menuitem-prefs">
                <property name="visible">True</property>
                <property name="label" translatable="yes" context="PhotoPlaceGUI" comments="menuitem">_Preferences</property>
                <property name="use_underline">True</property>
                <child type="submenu">
                  <object class="GtkMenu" id="menu-prefs">
                    <property name="visible">True</property>
                    <child>
                      <object class="GtkImageMenuItem" id="imagemenuitem-saveconf">
                        <property name="label">gtk-save</property>
                        <property name="visible">True</property>
                        <property name="tooltip_text" translatable="yes" context="PhotoPlaceGUI">Save the current preferences.</property>
                        <property name="use_underline">True</property>
                        <property name="use_stock">True</property>
                        <property name="always_show_image">True</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkSeparatorMenuItem" id="separatormenuitem-sep3">
                        <property name="visible">True</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkMenuItem" id="imagemenuitem-plugins">
                        <property name="visible">True</property>
                        <property name="tooltip_text" translatable="yes" context="PhotoPlaceGUI" comments="menuitem">List of available plugins. To see information about each plugin, put the mouse over its name,</property>
                        <property name="label" translatable="yes" context="PhotoPlaceGUI">_Plugins</property>
                        <property name="use_underline">True</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkMenuItem" id="menuitem-help">
                <property name="visible">True</property>
                <property name="label" translatable="yes" context="PhotoPlaceGUI" comments="menuitem">_Help</property>
                <property name="use_underline">True</property>
                <child type="submenu">
                  <object class="GtkMenu" id="menu-help">
                    <property name="visible">True</property>
                    <child>
                      <object class="GtkImageMenuItem" id="imagemenuitem-about">
                        <property name="label">gtk-about</property>
                        <property name="visible">True</property>
                        <property name="use_underline">True</property>
                        <property name="use_stock">True</property>
                        <property name="always_show_image">True</property>
                        <signal name="activate" handler="on_imagemenuitem-about_activate" object="aboutdialog"/>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="position">0</property>
          </packing>
        </child>
        <child>
          <object class="GtkToolbar" id="toolbar">
            <property name="visible">True</property>
            <property name="icon_size_set">True</property>
            <child>
              <object class="GtkToolItem" id="toolbutton-openphotos">
                <property name="visible">True</property>
                <property name="is_important">True</property>
                <child>
                  <object class="GtkButton" id="button-openphotos">
                    <property name="label" translatable="yes" context="PhotoPlaceGUI">Select input photo directory</property>
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <property name="receives_default">True</property>
                    <property name="has_tooltip">True</property>
                    <property name="tooltip_text" translatable="yes" context="PhotoPlaceGUI">Select a directory with photos for geotagging. All photos will be geotagged if &lt;geotagged&gt; mode is selected.</property>
                    <property name="image">image-photodir</property>
                    <property name="use_underline">True</property>
                    <signal name="clicked" handler="on_button-openphotos_clicked" object="button-openphotos"/>
                  </object>
                </child>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="homogeneous">True</property>
              </packing>
            </child>
            <child>
              <object class="GtkToolItem" id="toolbutton-opengpx">
                <property name="visible">True</property>
                <property name="is_important">True</property>
                <child>
                  <object class="GtkButton" id="button-opengpx">
                    <property name="label" translatable="yes" context="PhotoPlaceGUI">Input GPX file</property>
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <property name="receives_default">True</property>
                    <property name="has_tooltip">True</property>
                    <property name="tooltip_text" translatable="yes" context="PhotoPlaceGUI">Select an eXchange Format (GPX) in order to match the date of each photo with the position.</property>
                    <property name="image">image-gpx</property>
                    <property name="use_underline">True</property>
                    <signal name="clicked" handler="on_button-opengpx_clicked" object="button-opengpx"/>
                  </object>
                </child>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="homogeneous">True</property>
              </packing>
            </child>
            <child>
              <object class="GtkSeparatorToolItem" id="toolbutton-sep1">
                <property name="visible">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="homogeneous">True</property>
              </packing>
            </child>
            <child>
              <object class="GtkToggleToolButton" id="toggletoolbutton-plugins">
                <property name="visible">True</property>
                <property name="has_tooltip">True</property>
                <property name="tooltip_text" translatable="yes" context="PhotoPlaceGUI">If is selected, all the plugins will be activated.</property>
                <property name="label" translatable="yes" context="PhotoPlaceGUI">Plugins</property>
                <property name="use_underline">True</property>
                <property name="stock_id">gtk-convert</property>
                <property name="active">True</property>
                <signal name="toggled" handler="on_toggletoolbutton-plugins_toggled"/>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="homogeneous">True</property>
              </packing>
            </child>
            <child>
              <object class="GtkSeparatorToolItem" id="toolbutton-sep2">
                <property name="visible">True</property>
                <property name="visible_vertical">False</property>
                <property name="draw">False</property>
              </object>
              <packing>
                <property name="expand">True</property>
                <property name="homogeneous">True</property>
              </packing>
            </child>
            <child>
              <object class="GtkToolButton" id="toolbutton-exit">
                <property name="visible">True</property>
                <property name="has_tooltip">True</property>
                <property name="tooltip_text" translatable="yes" context="PhotoPlaceGUI">Exit the program. I hope see you soon :-)</property>
                <property name="label" translatable="yes" context="PhotoPlaceGUI">Exit</property>
                <property name="use_underline">True</property>
                <property name="stock_id">gtk-quit</property>
                <signal name="clicked" handler="on_toolbutton-exit_clicked"/>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="homogeneous">True</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="padding">5</property>
            <property name="position">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkVPaned" id="vpaned">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <child>
              <object class="GtkVBox" id="vbox1-vpaned">
                <property name="visible">True</property>
                <child>
                  <object class="GtkNotebook" id="notebook">
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <property name="scrollable">True</property>
                    <property name="tab_border">6</property>
                    <property name="tab_hborder">20</property>
                    <property name="tab_vborder">6</property>
                    <property name="homogeneous">True</property>
                    <child>
                      <object class="GtkFrame" id="frame-options">
                        <property name="visible">True</property>
                        <property name="label_xalign">0</property>
                        <child>
                          <object class="GtkVBox" id="vbox-options">
                            <property name="visible">True</property>
                            <property name="border_width">10</property>
                            <property name="spacing">8</property>
                            <child>
                              <object class="GtkHBox" id="hbox-out">
                                <property name="visible">True</property>
                                <child>
                                  <object class="GtkCheckButton" id="checkbutton-outgeo">
                                    <property name="visible">True</property>
                                    <property name="can_focus">True</property>
                                    <property name="receives_default">False</property>
                                    <property name="tooltip_text" translatable="yes" context="PhotoPlaceGUI" comments="geomode">If active, the exif data of pictures will be updated. With "write" mode each photo will be updated only if photo is not geotagged, by other hand "overwrite" will update each photo with the values calculated by PhotoPlace.</property>
                                    <property name="image">image-geo</property>
                                    <property name="focus_on_click">False</property>
                                    <property name="xalign">1</property>
                                    <property name="active">True</property>
                                    <property name="draw_indicator">False</property>
                                    <signal name="toggled" handler="on_checkbutton-outgeo_toggled" object="checkbutton-outgeo"/>
                                  </object>
                                  <packing>
                                    <property name="expand">False</property>
                                    <property name="position">0</property>
                                  </packing>
                                </child>
                                <child>
                                  <object class="GtkComboBox" id="combobox-exif">
                                    <property name="visible">True</property>
                                    <property name="model">liststore-exifmodes</property>
                                    <property name="active">0</property>
                                    <child>
                                      <object class="GtkCellRendererText" id="cellrenderertext-exif_name"/>
                                      <attributes>
                                        <attribute name="text">0</attribute>
                                      </attributes>
                                    </child>
                                  </object>
                                  <packing>
                                    <property name="expand">False</property>
                                    <property name="position">1</property>
                                  </packing>
                                </child>
                                <child>
                                  <object class="GtkLabel" id="label-and">
                                    <property name="visible">True</property>
                                    <property name="label" translatable="yes" context="PhotoPlaceGUI" comments="label separator between buttons in first page of notebook">and</property>
                                    <property name="use_markup">True</property>
                                    <property name="justify">center</property>
                                    <property name="single_line_mode">True</property>
                                  </object>
                                  <packing>
                                    <property name="expand">False</property>
                                    <property name="padding">10</property>
                                    <property name="position">2</property>
                                  </packing>
                                </child>
                                <child>
                                  <object class="GtkToggleButton" id="togglebutton-outfile">
                                    <property name="visible">True</property>
                                    <property name="can_focus">True</property>
                                    <property name="receives_default">True</property>
                                    <property name="tooltip_text" translatable="yes" context="PhotoPlaceGUI">If it is active, this will be the output file. It can be a KMZ (all in one, compressed Google Earth Layer) or KML and a directory with the processed photos.
Not active for geottaging purposes, no output file will be generated.</property>
                                    <property name="image">image-outfile</property>
                                    <property name="xalign">0</property>
                                    <signal name="toggled" handler="on_togglebutton-outfile_toggled" object="togglebutton-outfile"/>
                                  </object>
                                  <packing>
                                    <property name="position">3</property>
                                  </packing>
                                </child>
                              </object>
                              <packing>
                                <property name="expand">False</property>
                                <property name="padding">10</property>
                                <property name="position">0</property>
                              </packing>
                            </child>
                            <child>
                              <object class="GtkHBox" id="hbox-time">
                                <property name="visible">True</property>
                                <property name="spacing">5</property>
                                <property name="homogeneous">True</property>
                                <child>
                                  <object class="GtkHBox" id="hbox-utc">
                                    <property name="visible">True</property>
                                    <property name="spacing">10</property>
                                    <child>
                                      <object class="GtkLabel" id="label-utc">
                                        <property name="visible">True</property>
                                        <property name="has_tooltip">True</property>
                                        <property name="tooltip_text" translatable="yes" context="PhotoPlaceGUI" comments="label">The time zone of the photos. It depends on the settings of time/date of the camera and the GPS (UTC time), it is the difference in hours between both.</property>
                                        <property name="xalign">1</property>
                                        <property name="label" translatable="yes" context="PhotoPlaceGUI" comments="label">Time zone of photos:</property>
                                        <property name="use_markup">True</property>
                                        <property name="single_line_mode">True</property>
                                      </object>
                                      <packing>
                                        <property name="position">0</property>
                                      </packing>
                                    </child>
                                    <child>
                                      <object class="GtkSpinButton" id="spinbutton-utc">
                                        <property name="visible">True</property>
                                        <property name="can_focus">True</property>
                                        <property name="has_tooltip">True</property>
                                        <property name="tooltip_text" translatable="yes" context="PhotoPlaceGUI" comments="entry">The time zone of the photos. It depends on the settings of time/date of the camera and the GPS (UTC time), it is the difference in hours between both.</property>
                                        <property name="max_length">6</property>
                                        <property name="invisible_char">●</property>
                                        <property name="width_chars">8</property>
                                        <property name="secondary_icon_stock">gtk-home</property>
                                        <property name="adjustment">adjustment-utc</property>
                                        <property name="digits">2</property>
                                        <property name="numeric">True</property>
                                        <property name="update_policy">if-valid</property>
                                      </object>
                                      <packing>
                                        <property name="expand">False</property>
                                        <property name="fill">False</property>
                                        <property name="position">1</property>
                                      </packing>
                                    </child>
                                  </object>
                                  <packing>
                                    <property name="fill">False</property>
                                    <property name="position">0</property>
                                  </packing>
                                </child>
                                <child>
                                  <object class="GtkHBox" id="hbox-tdelta">
                                    <property name="visible">True</property>
                                    <property name="spacing">10</property>
                                    <child>
                                      <object class="GtkLabel" id="label-tdelta">
                                        <property name="visible">True</property>
                                        <property name="tooltip_text" translatable="yes" context="PhotoPlaceGUI" comments="label">The maximum difference in seconds allowed between the time of a photo and the nearest point of GPX.</property>
                                        <property name="xalign">1</property>
                                        <property name="label" translatable="yes" context="PhotoPlaceGUI">Maximum geotagging delta seconds:</property>
                                        <property name="use_markup">True</property>
                                      </object>
                                      <packing>
                                        <property name="position">0</property>
                                      </packing>
                                    </child>
                                    <child>
                                      <object class="GtkSpinButton" id="spinbutton-tdelta">
                                        <property name="visible">True</property>
                                        <property name="can_focus">True</property>
                                        <property name="has_tooltip">True</property>
                                        <property name="tooltip_text" translatable="yes" context="PhotoPlaceGUI" comments="entry">The maximum time difference allowed between the time of a photo and the nearest point of GPX.</property>
                                        <property name="max_length">6</property>
                                        <property name="invisible_char">●</property>
                                        <property name="width_chars">10</property>
                                        <property name="caps_lock_warning">False</property>
                                        <property name="secondary_icon_stock">gtk-justify-fill</property>
                                        <property name="adjustment">adjustment-tdelta</property>
                                        <property name="snap_to_ticks">True</property>
                                        <property name="numeric">True</property>
                                        <property name="update_policy">if-valid</property>
                                      </object>
                                      <packing>
                                        <property name="expand">False</property>
                                        <property name="fill">False</property>
                                        <property name="position">1</property>
                                      </packing>
                                    </child>
                                  </object>
                                  <packing>
                                    <property name="fill">False</property>
                                    <property name="position">1</property>
                                  </packing>
                                </child>
                              </object>
                              <packing>
                                <property name="expand">False</property>
                                <property name="position">1</property>
                              </packing>
                            </child>
                            <child>
                              <object class="GtkExpander" id="expander-advanced">
                                <property name="visible">True</property>
                                <property name="can_focus">True</property>
                                <property name="use_markup">True</property>
                                <child>
                                  <object class="GtkVBox" id="vbox-advanced">
                                    <property name="visible">True</property>
                                    <property name="spacing">10</property>
                                    <property name="homogeneous">True</property>
                                    <child>
                                      <object class="GtkHBox" id="hbox-photouri">
                                        <property name="visible">True</property>
                                        <property name="spacing">5</property>
                                        <property name="homogeneous">True</property>
                                        <child>
                                          <object class="GtkLabel" id="label-photouri">
                                            <property name="visible">True</property>
                                            <property name="tooltip_text" translatable="yes" context="PhotoPlaceGUI" comments="URI label">This entry defines the Uniform Resource Identifier for each geolocated photo. It is useful to generate a independent KML file where the photos will be linked into it.</property>
                                            <property name="xalign">1</property>
                                            <property name="label" translatable="yes" context="PhotoPlaceGUI">URI for geotagging photos:</property>
                                            <property name="selectable">True</property>
                                          </object>
                                          <packing>
                                            <property name="position">0</property>
                                          </packing>
                                        </child>
                                        <child>
                                          <object class="GtkEntry" id="entry-photouri">
                                            <property name="visible">True</property>
                                            <property name="can_focus">True</property>
                                            <property name="tooltip_text" translatable="yes" context="PhotoPlaceGUI" comments="entry URI">This entry defines the Uniform Resource Identifier for each geotagged photo. It is useful to generate a independent KML file where the photos will be linked into it.</property>
                                            <property name="max_length">256</property>
                                            <property name="invisible_char">●</property>
                                          </object>
                                          <packing>
                                            <property name="position">1</property>
                                          </packing>
                                        </child>
                                      </object>
                                      <packing>
                                        <property name="expand">False</property>
                                        <property name="position">0</property>
                                      </packing>
                                    </child>
                                    <child>
                                      <object class="GtkHBox" id="hbox-jpg">
                                        <property name="visible">True</property>
                                        <property name="spacing">5</property>
                                        <property name="homogeneous">True</property>
                                        <child>
                                          <object class="GtkHBox" id="hbox-quality">
                                            <property name="visible">True</property>
                                            <property name="border_width">5</property>
                                            <child>
                                              <object class="GtkLabel" id="label-morequality">
                                                <property name="visible">True</property>
                                                <property name="xalign">1</property>
                                                <property name="label" translatable="yes" context="PhotoPlaceGUI">Quality</property>
                                                <property name="use_markup">True</property>
                                                <property name="justify">right</property>
                                              </object>
                                              <packing>
                                                <property name="expand">False</property>
